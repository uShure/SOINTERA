{
  "name": "sointera-ai-sales-bot",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx src/telegram-userbot.ts",
    "start:bot": "tsx src/telegram-bot.ts",
    "seed": "tsx src/lib/database/seed.ts",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "demo:learning": "tsx src/scripts/demo-learning.ts",
    "analytics": "tsx -e \"import {LearningSytem} from './src/lib/ai/learning-system'; const ls = new LearningSytem(); ls.getDailySummary().then(console.log)\"",
    "learning:cycle": "tsx -e \"import {LearningSytem} from './src/lib/ai/learning-system'; const ls = new LearningSytem(); ls.learningEngine.performLearningCycle().then(() => console.log('Learning cycle completed'))\"",
    "voice:test": "tsx src/scripts/voice-test.ts",
    "voice:check": "tsx src/scripts/voice-check.ts",
    "voice:tts": "tsx src/scripts/voice-tts.ts",
    "voice:stt": "tsx src/scripts/voice-stt.ts",
    "voice:stats": "tsx src/scripts/voice-stats.ts"
  },
  "dependencies": {
    "@google-cloud/speech": "^7.2.0",
    "@google-cloud/text-to-speech": "^6.2.0",
    "@prisma/client": "^6.14.0",
    "dotenv": "^16.4.7",
    "espeak-ng": "^1.0.2",
    "fluent-ffmpeg": "^2.1.3",
    "https-proxy-agent": "^7.0.5",
    "node-fetch": "^2.7.0",
    "node-telegram-bot-api": "^0.65.1",
    "openai": "5.12.2",
    "openai-whisper": "^1.0.2",
    "telegram": "2.26.22",
    "tsx": "^4.20.0"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "24.2.1",
    "@types/node-telegram-bot-api": "^0.64.7",
    "prisma": "^6.14.0"
  }
}
